{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "DISCORD_CLIENT_ID": "@discord_client_id",
    "DISCORD_BOT_TOKEN": "@discord_bot_token",
    "MODE_ENV": "production"
  }
}
